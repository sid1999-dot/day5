        "    if (n==1):\r\n",
        "        return False\r\n",
        "    elif (n==2):\r\n",
        "        return True;\r\n",
        "    else:\r\n",
        "        for x in range(2,n):\r\n",
        "            if(n % x==0):\r\n",
        "                return False\r\n",
        "        return True             \r\n",
        "print(test_prime(9))"
